{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "judul": {
      "type": "string",
      "required": true
    },
    "isi": {
      "type": "richtext",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "deskripsi": {
      "type": "text",
      "required": true,
      "maxLength": 100,
      "minLength": 10
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "mappedBy": "articles"
    },
    "slug": {
      "type": "uid",
      "targetField": "judul",
      "required": true
    }
  }
}
